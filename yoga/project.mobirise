=\"Background Color\" value=\"#f4f9fc\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ff2121\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container' : !fullWidth, 'container-fluid' : fullWidth}\">\n        <div class=\"image\">\n            <div class=\"form__wrap\">\n                <div class=\"container\">\n                    <div class=\"inner__wrap\">\n                        <div class=\"row\" mbr-class=\"{'justify-content-center' : !showSubtitle || !showTitle || !showText || !showPhone, 'flex-row-reverse' : reverse}\">\n                            <div class=\"col-lg-6 subscribe__form\">\n                                    <div mbr-form class=\"d-flex\">\n                                        <!--Formbuilder Form-->\n                                        <form action name=\"Mobirise Form\" method=\"POST\" class=\"mbr-form form-with-styler\">\n                                        <div class=\"row\">\n                                        <div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Thanks for filling out the form!</div>\n                                        <div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\"> </div>\n                                        </div>\n                                        <div class=\"dragArea row\">\n                                        <div class=\"col-lg-12 col-md-12 col-sm-12 form-group\" data-for=\"e-mail\">\n                                        <input type=\"email\" name=\"e-mail\" placeholder=\"Email*\" data-form-field=\"email\" mbr-theme-style=\"display-7\" class=\"form-control\" value>\n                                        </div>\n                                        <div class=\"col-lg-12 col-md-12 col-sm-12 form-group\" data-for=\"message\">\n                                        <textarea name=\"message\" placeholder=\"Message\" data-form-field=\"message\" mbr-theme-style=\"display-7\" class=\"form-control\"></textarea>\n                                        </div>\n\n                                        <div class=\"col-lg-4 col-md-5 col-sm-5 input-group-btn d-flex\" mbr-buttons=\"true\" mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\"><a type=\"submit\" class=\"btn btn-md btn-primary\" data-app-placeholder=\"Type Text\">SEND</a></div>\n\n                                        </div>\n                                        </form><!--Formbuilder Form-->\n                                </div>\n                            </div>\n                            <div class=\"col-lg-6 title__block\" mbr-if=\"showSubtitle || showTitle || showText || showPhone\">\n                                    <h4 class=\"mbr-section-subtitle mbr-medium mbr-fonts-style\" mbr-if=\"showSubtitle\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-subtitle\">CONTACT US</h4>\n                                   <h3 class=\"mbr-section-title mbr-medium mbr-fonts-style\" mbr-if=\"showTitle\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\">Are you interested\n? Contact us.<br></h3>\n                                  <p class=\"mbr-text mbr-fonts-style\" mbr-if=\"showText\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Email us at or call this number</p>\n                                    <p class=\"phone mbr-fonts-style\" mbr-if=\"showPhone\" mbr-theme-style=\"display-5\" data-app-selector=\".phone\">+(123)456-78-90</p>\n              \n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n       </div>\n        \n    </div>\n</section>",

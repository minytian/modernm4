          "_name": "footer1",
          "_customHTML": "<section class=\"footer1\" group=\"Footer\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" always-bottom global>\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"checkbox\" title=\"Show Animation\" name=\"showAnimation\" checked>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Show Brand\" name=\"showBrand\" checked>\n        <select name=\"typeMenu\" title=\"Type Menu\" condition=\"showBrand\">\n            <option value=\"1\">Logo Text</option>\n            <option value=\"2\" selected>Logo Image</option>\n        </select>\n        <input type=\"range\" inline title=\"Width Logo\" name=\"widthLogo\" min=\"20\" max=\"100\" step=\"2\" value=\"60\" condition=\"showBrand && typeMenu == 2\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\">\n        <input type=\"range\" inline title=\"Menu Items 1\" name=\"menuItems\" min=\"0\" max=\"6\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Menu Items 2\" name=\"menuItems2\" min=\"0\" max=\"6\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline title=\"Social Icons\" name=\"socIcons\" min=\"0\" max=\"4\" step=\"1\" value=\"3\">\n        <input type=\"color\" title=\"Icon Color\" value=\"#dbdbdb\" name=\"iconColor\" condition=\"socIcons > 0\">\n        <hr>\n        <input type=\"color\" title=\"Divider Color\" value=\"#000000\" name=\"dividerColor\" condition=\"showBottom\">\n        <input type=\"checkbox\" title=\"Show Bottom\" name=\"showBottom\">\n        <input type=\"range\" inline title=\"Menu Bottom Items\" name=\"menuBottomItems\" min=\"0\" max=\"6\" step=\"1\" valu